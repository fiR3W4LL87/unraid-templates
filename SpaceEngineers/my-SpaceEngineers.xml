  <Config Name="SteamCMD" Target="/serverdata/steamcmd" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/steamcmd/</Config>
  <Config Name="ServerFiles" Target="/appdata/space-engineers/SpaceEngineersDedicated" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/SpaceEngineersDedicated</Config>
  <Config Name="ServerConfig" Target="/appdata/space-engineers/instances" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/SpaceEngineersDedicated/Instances</Config>
